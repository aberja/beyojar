{
    "expo": {
        "name": "Bejojar",
        "description": "An open source notes application",
        "slug": "bejojar",
        "owner": "kajendran",
        "version": "1.0.0",
        "githubUrl": "https://github.com/kaje94/beyojar",
        "primaryColor": "#7AB17C",
        "orientation": "portrait",
        "icon": "./src/assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./src/assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#7AB17C"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/images/adaptive-icon.png",
                "backgroundColor": "#7AB17C"
            }
        },
        "web": {
            "favicon": "./src/assets/images/favicon.png",
            "bundler": "metro"
        }
    }
}
